{
  "GlobalConfiguration": {
    "ServiceDiscoveryProvider": {
      "Host": "localhost",
      "Port": 8500,
      "Type": "Consul"
    }
  },
  "Routes": [
    {
      "ServiceName": "comms",
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/api/comms/{everything}",
      "UpstreamHttpMethod": [ "Get", "Options", "Post", "Put", "Delete" ],
      "RouteIsCaseSensitive": false
    },
    {
      "ServiceName": "content",
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/api/content/{everything}",
      "UpstreamHttpMethod": [ "Get", "Options", "Post", "Put", "Delete" ],
      "RouteIsCaseSensitive": false
    },
    {
      "ServiceName": "identity",
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/api/identity/{everything}",
      "UpstreamHttpMethod": [ "Get", "Options", "Post", "Put", "Delete" ],
      "RouteIsCaseSensitive": false
    },
    {
      "ServiceName": "reports",
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/api/reports/{everything}",
      "UpstreamHttpMethod": [ "Get", "Options", "Post", "Put", "Delete" ],
      "RouteIsCaseSensitive": false
    },
    {
      "ServiceName": "sockets",
      "DownstreamPathTemplate": "/hubs/{hub}/negotiate",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/sockets/hubs/{hub}/negotiate",
      "UpstreamHttpMethod": [ "Options", "Post" ],
      "RouteIsCaseSensitive": false
    },
    {
      "ServiceName": "sockets",
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "ws",
      "UpstreamPathTemplate": "/sockets/{everything}",
      "UpstreamHttpMethod": [ "Get", "Options", "Post" ],
      "RouteIsCaseSensitive": false
    }
  ],
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedOrigins": "",
  "AllowedHosts": "*"
}